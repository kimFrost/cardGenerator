
namespace MainApp {

	export function appRun($templateCache:ng.ITemplateCacheService) {

		$templateCache.put('validation-messages', HtmlTemplates.validationMessages.partial.html);
		$templateCache.put('email-validation-messages', HtmlTemplates.emailValidationMessages.partial.html);

		// Generic form partials
		$templateCache.put('form-email', HtmlTemplates.form.email.partial.html);
	}
}
